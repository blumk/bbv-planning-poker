    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@ionic/core": "4.0.0-beta.7",
    "@stencil/core": "0.12.4"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blumk/bbv-planning-poker.git"
  },
  "author": "Ken Blum",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blumk/bbv-planning-poker"
  },
  "homepage": "https://github.com/blumk/bbv-planning-poker",
  "jest": {
    "transform": {
      "^.+\\.(js|ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
